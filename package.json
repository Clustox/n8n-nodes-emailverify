{
  "name": "n8n-nodes-emailverify",
  "version": "1.0.2",
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "description": "n8n node to verify emails using EmailVerify.io",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node-package",
    "emailverify",
    "emailfinder",
    "email",
    "verification",
    "validation"
  ],
  "license": "MIT",
  "homepage": "https://emailverify.io",
  "author": {
    "name": "Clustox <support@emailverify.io> (https://www.emailverify.io)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Clustox/n8n-nodes-emailverify"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc -w",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/EmailVerifyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/EmailVerify/EmailVerify.node.js"
    ]
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.16",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^5.0.0",
    "prettier": "^2.8.8",
    "typescript": "~5.1.3"
  },
  "dependencies": {
    "axios": "^1.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": ">1.7.0"
  }
}
